<%= simple_form_for(@product) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :cate_id %>
    <%= f.association :region %>
    <%= f.association :city %>
    <%= f.association :district %>
    <%= f.input :title %>
    <%= f.input :content %>
    <%= f.input :price %>
    <%= f.input :detail_address %>
    <%= f.input :pv %>
    <%= f.input :is_processed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
