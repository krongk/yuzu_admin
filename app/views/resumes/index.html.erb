<h1>Listing resumes</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>User</th>
      <th>Cate</th>
      <th>Region</th>
      <th>City</th>
      <th>District</th>
      <th>Name</th>
      <th>Summary</th>
      <th>Sex</th>
      <th>Age</th>
      <th>Education</th>
      <th>Work year</th>
      <th>Content</th>
      <th>Mobile phone</th>
      <th>Mobile phone str</th>
      <th>Email</th>
      <th>Qq</th>
      <th>Pv</th>
      <th>Fav</th>
      <th>Is processed</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @resumes.each do |resume| %>
      <tr>
        <td><%= link_to resume.user.name, user_path(resume.user) %></td>
        <td><%= resume.cate_id %></td>
        <td><%= link_to resume.region.try(:name), region_path(resume.region) %></td>
        <td><%= link_to resume.city.try(:name), city_path(resume.city) %></td>
        <td><%= link_to resume.district.try(:name), district_path(resume.district) %></td>
        <td><%= resume.name %></td>
        <td><%= resume.summary %></td>
        <td><%= resume.sex %></td>
        <td><%= resume.age %></td>
        <td><%= resume.education %></td>
        <td><%= resume.work_year %></td>
        <td><%= resume.content %></td>
        <td><%= resume.mobile_phone %></td>
        <td><%= resume.mobile_phone_str %></td>
        <td><%= resume.email %></td>
        <td><%= resume.qq %></td>
        <td><%= resume.pv %></td>
        <td><%= resume.fav %></td>
        <td><%= resume.is_processed %></td>
        <td><%= link_to 'Show', resume %></td>
        <td><%= link_to 'Edit', edit_resume_path(resume) %></td>
        <td><%= link_to 'Destroy', resume, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resume', new_resume_path %>
